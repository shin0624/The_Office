{
  "dialogues": [
    {
      "id": 1,
      "boss_type": "male",
      "speaker": "boss",
      "dialogue_text": "오늘 회의 준비는 다 됐나?",
      "choices": [
        {
          "choice_id": 1,
          "choice_text": "네, 완벽하게 준비했습니다!",
