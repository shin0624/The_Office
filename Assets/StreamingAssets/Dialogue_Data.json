        {
          "choice_id": 1,
          "choice_text": "감사합니다! 많이 배우고 있습니다.",
          "affection_change": 10,
          "social_score_change": 14
        },
        {
          "choice_id": 2,
          "choice_text": "사회생활 힘드네요.",
          "affection_change": 2,
          "social_score_change": 5
        },
        {
          "choice_id": 3,
          "choice_text": "눈이 있으면 좀 보세요 누구때문에 힘든지",
          "affection_change": -15,
          "social_score_change": -8
        }
      ]
    },
    {
      "id": 5,
      "boss_type": "male",
      "speaker": "boss",
      "dialogue_text": "오늘 야근해야 할 것 같은데, 괜찮나? 중요한 마감이 있어서 말이야.",
      "choices": [
        {
          "choice_id": 1,
          "choice_text": "네, 물론입니다! 끝까지 함께하겠습니다.",
          "affection_change": 15,
          "social_score_change": 20
