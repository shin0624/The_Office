        {
          "choice_id": 2,
          "choice_text": "알아주시니 감사합니다.",
          "affection_change": 3,
          "social_score_change": 7
        },
        {
          "choice_id": 3,
          "choice_text": "알면 좀 챙겨주세요 맨날 눈치만 주지 마시고",
          "affection_change": -25,
          "social_score_change": 2
        }
      ]
    },
    {
      "id": 7,
      "boss_type": "male",
      "speaker": "boss",
      "dialogue_text": "내일 중요한 고객 미팅이 있는데, 자료 준비 도와줄 수 있나?",
      "choices": [
        {
          "choice_id": 1,
